
generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url = env("DATABASE_URL")
}

model Player {
	createdAt		DateTime	@default(now())
	updatedAt		DateTime	@updatedAt

	id				Int			@id
	username		String		@unique
	avatar			String		@unique

	firstName		String ?
	lastName		String ?
	profileIntra	String ?

	twofaSecret		String ?

}
