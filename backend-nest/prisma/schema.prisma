
generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "postgresql"
	url = env("DATABASE_URL")
}

model Player {
	createdAt		DateTime	@default(now())
	updatedAt		DateTime	@updatedAt

	id				Int			@id
	username		String		@unique
	avatar			String		@unique
	prova			String
	prova2			Int
	firstName		String ?
	lastName		String ?
	profileIntra	String ?

}
